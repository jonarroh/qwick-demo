generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Post{
  id    String @id @default(cuid())
  createdAt DateTime @default(now())
  content String @db.VarChar(255)
  authorId String @db.VarChar(255)
  autorName String @db.VarChar(255)

  @@index([authorId])

}